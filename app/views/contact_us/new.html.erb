<h1>Contact Us</h1>

<%= form_tag contact_us_path do%>
<div>
  <%= label_tag :full_name %>
  <%= text_field_tag :full_name, nil, {placeholder: "ex. John Smith"} %>
</div>
<div>
  <%= label_tag :email %>
  <%= email_field_tag :email, nil, {placeholder: "ex. john@example.org"}%>
</div>
<div>
  <%= label_tag :messages %>
  <%= text_area_tag :messages %>
</div>
<div>
  <%= submit_tag "Contact Us!" %>
</div>
<% end %>
